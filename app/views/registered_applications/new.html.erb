<h1>RegisteredApplications#new</h1>
<p>Find me in app/views/registered_applications/new.html.erb</p>

<%= form_for @registered_apps do |f| %>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>

    </br>

    <div>
      <%= f.label :url %>
      <%= f.text_field :url %>
    </div>

    </br>

    <div>
      <%= f.submit "Save" %>
    </div>
<% end %>
